{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"semi": ["error", "always"],
		"indent": ["error", 2],
		"quotes": ["error", "double"],
		"no-underscore-dangle": "off"
	}
}

